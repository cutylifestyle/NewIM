<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <android.support.v7.widget.AppCompatImageView
        android:id="@+id/iv_1"
        android:layout_width="@dimen/x150"
        android:layout_height="@dimen/x75"
        android:layout_marginStart="@dimen/x16"
        android:layout_marginTop="@dimen/x60"
        android:contentDescription="@string/app_name"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/intro1_item_1"
        app:x_in="0.8"
        app:x_out="0.8" />

    <android.support.v7.widget.AppCompatImageView
        android:id="@+id/iv_2"
        android:layout_width="@dimen/x125"
        android:layout_height="@dimen/x55"
        android:layout_marginTop="@dimen/x125"
        android:contentDescription="@string/app_name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/intro1_item_2"
        app:x_in="1.1"
        app:x_out="1.1"/>

    <android.support.v7.widget.AppCompatImageView
        android:id="@+id/iv_3"
        android:layout_width="@dimen/x40"
        android:layout_height="@dimen/x40"
        android:layout_marginBottom="@dimen/x180"
        android:layout_marginEnd="@dimen/x50"
        android:contentDescription="@string/app_name"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/intro1_item_3"
        app:x_in="0.8"
        app:x_out="0.8"
        app:a_in="0.8"
        app:a_out="0.8"/>

    <android.support.v7.widget.AppCompatImageView
        android:id="@+id/iv_4"
        android:layout_width="0dp"
        android:layout_height="@dimen/x120"
        android:layout_marginBottom="@dimen/x30"
        android:background="@drawable/intro1_item_4"
        android:contentDescription="@string/app_name"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:a_in="0.8"
        app:a_out="0.8"
        app:x_in="0.8"
        app:x_out="0.8" />

    <android.support.v7.widget.AppCompatImageView
        android:id="@+id/iv_5"
        android:layout_width="@dimen/x240"
        android:layout_height="@dimen/x18"
        android:layout_marginBottom="@dimen/x20"
        android:layout_marginStart="@dimen/x16"
        android:contentDescription="@string/app_name"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/intro1_item_5"
        app:a_in="0.9"
        app:a_out="0.9"
        app:x_in="0.9"
        app:x_out="0.9" />

    <android.support.v7.widget.AppCompatImageView
        android:id="@+id/iv_6"
        android:layout_width="@dimen/x24"
        android:layout_height="@dimen/x110"
        android:layout_marginBottom="@dimen/x32"
        android:layout_marginStart="@dimen/x48"
        android:contentDescription="@string/app_name"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/intro1_item_6"
        app:x_in="0.6"
        app:x_out="0.6"/>

    <android.support.v7.widget.AppCompatImageView
        android:id="@+id/iv_7"
        android:layout_width="@dimen/x45"
        android:layout_height="@dimen/x40"
        android:layout_marginBottom="@dimen/x32"
        android:layout_marginStart="@dimen/x80"
        android:contentDescription="@string/app_name"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/intro1_item_7"
        app:a_in="0.3"
        app:a_out="0.3"
        app:x_in="0.5"
        app:x_out="0.5"/>
</android.support.constraint.ConstraintLayout>